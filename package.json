{
  "name": "z-axios",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/zpbc007/z-axios.git",
  "author": "zhaopeng <719197352@qq.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "min": "terser dist/index.js -o dist/index.min.js",
    "build": "npm run clean:dist && rollup -c && npm run min",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint:fix": "tslint -c tslint.json \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/*.{js,ts}": [
      "npm run format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.11",
    "axios": "^0.18.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "terser": "^3.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "url": "^0.11.0"
  }
}
